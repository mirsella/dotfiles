{
	"$schema": "https://opencode.ai/config.json",
	"plugin": [
		"opencode-antigravity-auth@1.4.5-beta.0",
		"@tarquinen/opencode-dcp@1.2.8",
		"@tarquinen/opencode-smart-title@0.1.7",
		"opencode-supermemory@0.1.6"
	],
	"disabled_providers": ["openrouter"],
	"formatter": {
		"rustfmt": {
			"command": ["rustfmt", "$FILE"],
			"extensions": [".rs"]
		}
	},
	"keybinds": {
		"model_cycle_favorite": "f4",
		"model_cycle_favorite_reverse": "shift+f4"
	},
	"agent": {
		"explore": {
			"model": "google/gemini-3-flash-preview",
			"reasoningEffort": "high"
		}
	},
	"mcp": {
		"context7": {
			"type": "remote",
			"url": "https://mcp.context7.com/mcp"
		}
	},
	"lsp": {
		"rust": {
			"disabled": true
		},
		"rust-lspmux": {
			"command": ["lspmux", "client"],
			"extensions": [".rs"]
		}
	},
	"permission": {
		"external_directory": "allow",
		"bash": {
			"sudo*": "deny",
			"chmod*": "deny",
			"chown*": "deny",
			"git reset*": "ask",
			"git checkout*": "ask",

			"brew*": "deny",
			"paru*": "deny",
			"pacman*": "deny"
		}
	},

	"provider": {
		// "github-copilot": {
		// 	"npm": "@opeoginni/github-copilot-openai-compatible",
		// 	"name": "GitHub Copilot",
		// 	"id": "github-copilot"
		// }
		"kimi-for-coding": {
			"models": {
				"k2p5": {
					"variants": {
						"max": {
							"thinking": {
								"type": "enabled",
								"budgetTokens": 16000
							}
						}
					}
				}
			}
		},
		"github-copilot": {
			"models": {
				"gpt-5.2-codex": {
					"name": "GPT-5.2 Codex",
					"limit": {
						// "context": 400000,
						"context": 300000,
						"output": 128000
						// "input": 272000
					}
				},
				"gpt-5.2": {
					"name": "GPT-5.2",
					"limit": {
						// "context": 400000,
						"context": 300000,
						"output": 128000
						// "input": 272000
					}
				},
				"claude-opus-4.5": {
					"name": "Claude Opus 4.5",
					"limit": { "context": 160000, "input": 128000, "output": 16000 }
				}
			}
		},
		"google": {
			"models": {
				"antigravity-gemini-3-pro": {
					"name": "Gemini 3 Pro (Antigravity)",
					"limit": { "context": 1048576, "output": 65535 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					},
					"variants": {
						"low": { "thinkingLevel": "low" },
						"high": { "thinkingLevel": "high" }
					}
				},
				"antigravity-gemini-3-flash": {
					"name": "Gemini 3 Flash (Antigravity)",
					"limit": { "context": 1048576, "output": 65536 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					},
					"variants": {
						// "minimal": { "thinkingLevel": "minimal" },
						"low": { "thinkingLevel": "low" },
						// "medium": { "thinkingLevel": "medium" },
						"high": { "thinkingLevel": "high" }
					}
				},
				"antigravity-claude-sonnet-4-5": {
					"name": "Claude Sonnet 4.5 (Antigravity)",
					"limit": { "context": 200000, "output": 64000 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"antigravity-claude-sonnet-4-5-thinking": {
					"name": "Claude Sonnet 4.5 Thinking (Antigravity)",
					"limit": { "context": 200000, "output": 64000 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					},
					"variants": {
						"low": { "thinkingConfig": { "thinkingBudget": 8192 } },
						"max": { "thinkingConfig": { "thinkingBudget": 32768 } }
					}
				},
				"antigravity-claude-opus-4-5-thinking": {
					"name": "Claude Opus 4.5 Thinking (Antigravity)",
					"limit": { "context": 200000, "output": 64000 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					},
					"variants": {
						"low": { "thinkingConfig": { "thinkingBudget": 8192 } },
						"max": { "thinkingConfig": { "thinkingBudget": 32768 } }
					}
				},
				"gemini-2.5-flash": {
					"name": "Gemini 2.5 Flash (Gemini CLI)",
					"limit": { "context": 1048576, "output": 65536 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"gemini-2.5-pro": {
					"name": "Gemini 2.5 Pro (Gemini CLI)",
					"limit": { "context": 1048576, "output": 65536 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"gemini-3-flash-preview": {
					"name": "Gemini 3 Flash Preview (Gemini CLI)",
					"limit": { "context": 1048576, "output": 65536 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"gemini-3-pro-preview": {
					"name": "Gemini 3 Pro Preview (Gemini CLI)",
					"limit": { "context": 1048576, "output": 65535 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				}
			}
		}
	}
}
