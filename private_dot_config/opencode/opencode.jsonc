{
	"$schema": "https://opencode.ai/config.json",
	"share": "disabled",
	"formatter": {
		"rustfmt": {
			"command": ["rustfmt", "$FILE"],
			"extensions": [".rs"]
		}
	},
	"keybinds": {
		"model_cycle_recent": "f4",
		"model_cycle_recent_reverse": "shift+f4"
	},
	"mcp": {
		"deepwiki": {
			"type": "remote",
			"url": "https://mcp.deepwiki.com/mcp",
			"enabled": true
		}
	}
}
